<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>

    <link rel = "stylesheet" href="css/mco.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <title>
      <%= title %>
    </title>

    <style></style>
</head>
<body>
    <%- include('partials/adminHeader') -%>



<!-- homepage -->


<div class="row page">
    <div class="page text-center bg-light">
                <h1>DASHBOARD</h1>
                <a href ="/accountList" class ="btn btn-danger btn-sm remove">Accounts List</a>


    </div>
    <div class="reservation-status">
    <div class="page text-center bg-light">
            <h2> Manager List </h2>
              <table class="account" id="account">
                <thead>
                  <tr>
                      <th>Name</th>
                      <th>Username</th>
                      <th>Email</th>
                      <th>Phone</th>
                      <th>Restaurant</th>
                      <th>Edit Account</th>

                  </tr>
              </thead>
              <tbody>
                <% managers.forEach(function(managers){ %>
                  <tr>
                    <td><%= managers.name%></td>
                    <td><%= managers.username%></td>
                    <td><%= managers.email%></td>
                    <td><%= managers.phone%></td>

                    <form action="/manageChange" method="post">
                      <input type="hidden" name = "username" id = "username" value = <%= managers.username%> >
                      <td><select name = "restaurant" id = "restaurant" >
                      <!-- if value == "Manager"-->
                      <option disabled selected hidden  value = <%= managers.restaurant%> ><%= managers.restaurant%></option>
                      <!-- <option disabled selected hidden  value = <%= managers.branch%> ><%= managers.branch%></option> -->
                      <option value = "Kuya J">Kuya J</option>
                      <option value = "Gerry's Grill">Gerry's Grill</option>
                      <option value = "Max's Restaurant">Max's Restaurant</option>
                      </select></td>
                      <td><button type ="submit" class ="btn btn-danger btn-sm remove">Update</button></td>
                    </form>
                    
                </tr>
                  <% }); %>
                  

                </tbody>
              </table>

              </div>
          
    </div>
    
      
</div>
</div>
</div>



<!-- footer-->
<%- include('partials/footer') -%>

<!-- End of .container -->
<!-- Scripts -->



</body>
</html>
