<!DOCTYPE html>

<html>
<head>

    <title><%= title %></title>
    
    <!--  Style Links  -->
    <link rel = "stylesheet" href="css/mco.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>

<body>
    <!--Container Nav-->
    <%- include('partials/loginheader') -%>



<!-- 5 sample posts -->
<div class="reservation-status page">
    <h2> Your Reservations </h2>
      <table class="reservation-details" id="reservation-details">
        <thead>
          <tr>
              <th>Restaurant</th>
              <th>Branch</th>
              <th>Date</th>
              <th>Check-in</th>
              <th>Number of People</th>
              <th>Status</th>

          </tr>
      </thead>

      <tbody>
        <% reservations.forEach(function(reservations){ %>
            <tr>
                <td><%= reservations.restaurant%></td>
                <td><%= reservations.branch%></td>
                <td><%= reservations.datein%></td>
                <td><%= reservations.timein%></td>
                <td><%= reservations.numpeople%></td>
                <td><%= reservations.status%></td>

            </tr>
            <% }); %>
            </tbody>
          </table>


          <a href="/add" class="btn btn-sm btn-primary">Reserve Now!!</a> <br/><br/>

          
    </div>



    <%- include('partials/footer') -%>
</body>
</html>
